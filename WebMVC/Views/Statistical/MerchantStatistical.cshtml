@model List<WebMVC.EntityFramework.MERCHANT>
@{
    ViewBag.Title = "MerchantStatistical";
}
<h2>MerchantSumarDailyStatistical</h2>
<table class="table table-striped table-bordered table-hover">
    <thead>
    <th>MerchantCode</th>
    <td>MerchantName</td>
    <td>Owner</td>
    <td>MerchantType</td>
    <td>Address1</td>
    <td>CityCode</td>
    <td>Zip</td>
    <td>Phone</td>
    <td>Email</td>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(model => item.MerchantCode)</td>
                <td>@Html.DisplayFor(model => item.MerchantName)</td>
                <td>@Html.DisplayFor(model => item.Owner)</td>
                <td>@Html.DisplayFor(model => item.MerchantType)</td>
                <td>@Html.DisplayFor(model => item.Address1)</td>
                <td>@Html.DisplayFor(model => item.CityCode)</td>
                <td>@Html.DisplayFor(model => item.Zip)</td>
                <td>@Html.DisplayFor(model => item.Phone)</td>
                <td>@Html.DisplayFor(model => item.Email)</td>

            </tr>
        }
    </tbody>
</table>

