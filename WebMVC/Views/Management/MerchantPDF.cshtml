@model List<WebMVC.EntityFramework.MERCHANT>
@{
    ViewBag.Title = "ExportMerchantPDF";
}
<h2 size="20" align="center" style="font-size: 40px;font-family: Arial">>MERCHANT MANAGEMENT</h2>
<hr />
<table class="table table-striped table-bordered table-hover">
    <tbody>
    <td style="font-size: 20px"><strong>MerchantCode</strong></td>
    <td style="font-size: 20px"><strong>MerchantName</strong></td>
    <td style="font-size: 20px"><strong>MerchantType</strong></td>
    <td style="font-size: 20px"><strong>Status</strong></td>
    <td style="font-size: 20px"><strong>Owner</strong></td>
    <td style="font-size: 20px"><strong>Address</strong></td>
    <td style="font-size: 20px"><strong>City</strong></td>
    <td style="font-size: 20px"><strong>LastActiveDate</strong></td>
    <td style="font-size: 20px"><strong>ClosedDate</strong></td>
    </tbody>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(model => item.MerchantCode )</td>
                <td>@Html.DisplayFor(model => item.MerchantName )</td>
                <td>@Html.DisplayFor(model => item.MerchantType )</td>
                <td>@Html.DisplayFor(model => item.Status )</td>
                <td>@Html.DisplayFor(model => item.Owner )</td>
                <td>@Html.DisplayFor(model => item.Address1 )</td>
                <td>@Html.DisplayFor(model => item.CityName )</td>
                <td>@Html.DisplayFor(model => item.LastActiveDate)</td>
                <td>@Html.DisplayFor(model => item.CloseDate)</td>
            </tr>
        }
    </tbody>
</table>



