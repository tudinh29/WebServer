<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<sql-query name="SP_GetReportDataForLineChart_Generality">
        <return-scalar column="name" type="string" />
        <return-scalar column="value" type="big_decimal" />

        SP_GetReportDataForLineChart_Generality :startDate, :endDate
    </sql-query>
    
    <sql-query name="SP_GetReportDataForLineChart_Default">
        <return-scalar column="name" type="string" />
        <return-scalar column="value" type="big_decimal" />

        SP_GetReportDataForLineChart_Default
    </sql-query>
</hibernate-mapping>